/ {
    macros {
        macro_period_space_shift: macro_period_space_shift {
            label = "Period, Space, Shift";
            compatible = "zmk,behavior-macro";
            bindings = <&kp DOT &kp SPACE &sk LSFT>;
            #binding-cells = <0>;
        };
        macro_fun_stickyshift: macro_fun_stickyshift {
            label = "Fun StickyShift";
            compatible = "zmk,behavior-macro";
            bindings = <&mo FUN> <&sk LSFT>;
            #binding-cells = <0>;
        };
    };
};
